{
	"name": "pelicanode",
	"version": "1.0.0",
	"description": "Fully-typed Node.js client for the Pelican Panel API (Application + Client APIs)",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsc",
		"clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\"",
		"prepublishOnly": "npm run clean && npm run build",
		"lint": "eslint src",
		"format": "prettier --write src"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/H7KZ/Pelicanode.git"
	},
	"keywords": [
		"pelican",
		"pelican-panel",
		"game-server",
		"api",
		"client",
		"typescript"
	],
	"author": "H7KZ",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/H7KZ/Pelicanode/issues"
	},
	"homepage": "https://github.com/H7KZ/Pelicanode",
	"engines": {
		"node": ">=18.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^10.0.1",
		"@ianvs/prettier-plugin-sort-imports": "^4.7.1",
		"@types/node": "^24.2.1",
		"eslint": "^10.0.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-import": "^2.32.0",
		"globals": "^17.3.0",
		"prettier": "^3.8.1",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.56.0"
	},
	"dependencies": {
		"axios": "^1.11.0"
	}
}
