{
	"name": "pelicanode",
	"version": "1.0.0-beta33-2",
	"description": "Fully-typed Node.js client for the Pelican Panel API (Application + Client APIs)",
	"type": "module",
	"sideEffects": false,
	"packageManager": "npm@10.9.2",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsup",
		"clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\"",
		"prepublishOnly": "npm run clean && npm run build",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:types": "vitest --typecheck run",
		"lint": "eslint src",
		"format": "prettier --write src"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/H7KZ/Pelicanode.git"
	},
	"keywords": [
		"pelican",
		"pelican-panel",
		"game-server",
		"api",
		"client",
		"typescript"
	],
	"author": "H7KZ",
	"license": "AGPL-3.0-only",
	"bugs": {
		"url": "https://github.com/H7KZ/Pelicanode/issues"
	},
	"homepage": "https://github.com/H7KZ/Pelicanode",
	"engines": {
		"node": ">=18.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.3",
		"@ianvs/prettier-plugin-sort-imports": "^4.7.1",
		"@types/node": "^24.10.13",
		"axios-mock-adapter": "^2.1.0",
		"eslint": "^9.39.3",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-import-x": "^4.16.1",
		"globals": "^17.3.0",
		"prettier": "^3.8.1",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.0",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"axios": "^1.13.5"
	}
}
